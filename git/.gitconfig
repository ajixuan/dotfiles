[user]
	name = Allen Ji
	email = ajixuan11@gmail.com
[alias]
        a = add -p
        r = reset
        st = status
        br = branch
        co = checkout
        ci = commit
        ps = push
        pl = pull
        d = diff
        dc = diff --cached
        cp = cherry-pick
        rv = rev-parse HEAD
        cl = clone
[core]
	hooksPath = .githooks
	editor = nvim
[rebase]
	autosquash = true
[push]
	default = current
[credential]
	helper = cache --timeout=3600
[pull]
	rebase = false
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
